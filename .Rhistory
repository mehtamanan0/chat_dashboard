utc_date_dtring <- as.POSIXct(date_string, tz="UTC")
df[colname] <- format(utc_date_dtring, tz="Asia/Kolkata",usetz=TRUE)
df[colname] <- strftime(df[colname], "%Y-%m-%d %H:%M:%S")
return(df)
}
